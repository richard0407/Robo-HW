/*N Factorial*/

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner scan = new Scanner(System.in);
    System.out.println("Enter an integer: ");
    int x = scan.nextInt();
    int y = x;
    //System.out.println("\n" + x);
    for(int i = x; i > 1; i--){
      x -= 1;
      y *= x;
      //System.out.println(y);
      if(x == 1){
        System.out.println(y);
      }
    }
  }
}
